<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activity_welcome"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:text="@string/all_placeholder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/reporter"
        android:layout_alignBaseline="@+id/textView18"
        android:layout_alignBottom="@+id/textView18"
        android:layout_alignStart="@+id/reportNum" />

    <TextView
        android:text="@string/all_placeholder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/location"
        android:layout_alignBaseline="@+id/textView20"
        android:layout_alignBottom="@+id/textView20"
        android:layout_alignEnd="@+id/textView13" />

    <TextView
        android:text="@string/all_placeholder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/date"
        android:layout_alignBaseline="@+id/textView19"
        android:layout_alignBottom="@+id/textView19"
        android:layout_alignStart="@+id/location" />

    <TextView
        android:text="@string/all_placeholder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView102"
        android:layout_alignEnd="@+id/textView101"
        android:id="@+id/textView103" />

    <TextView
        android:text="@string/all_placeholder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/reportNum"
        android:layout_alignStart="@+id/location"
        android:layout_alignTop="@+id/textView17"
        android:layout_above="@+id/textView18" />

    <TextView
        android:text="@string/all_reporter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:id="@+id/textView18"
        android:layout_below="@+id/textView17"
        android:layout_alignStart="@+id/textView17" />

    <TextView
        android:text="@string/all_date_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="21dp"
        android:id="@+id/textView19"
        android:layout_below="@+id/textView18"
        android:layout_alignStart="@+id/textView18" />

    <TextView
        android:text="@string/historical_report_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:id="@+id/textView20"
        android:layout_below="@+id/textView19"
        android:layout_alignStart="@+id/textView19" />

    <TextView
        android:text="@string/all_placeholder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:id="@+id/textView104"
        android:layout_below="@+id/textView102"
        android:layout_alignStart="@+id/textView102" />

    <TextView
        android:text="@string/all_report"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView13"
        android:textSize="36sp"
        android:layout_marginTop="44dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="@string/all_report_no"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="70dp"
        android:id="@+id/textView17"
        android:layout_below="@+id/textView13"
        android:layout_toStartOf="@+id/textView13" />

    <TextView
        android:text="@string/all_placeholder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:id="@+id/textView102"
        android:layout_below="@+id/textView100"
        android:layout_alignEnd="@+id/textView17" />

    <TextView
        android:text="@string/all_placeholder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:id="@+id/textView100"
        android:layout_below="@+id/textView20"
        android:layout_alignStart="@+id/textView102" />

    <TextView
        android:text="@string/all_placeholder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView101"
        android:layout_alignBaseline="@+id/textView100"
        android:layout_alignBottom="@+id/textView100"
        android:layout_alignStart="@+id/location" />

    <TextView
        android:text="@string/all_placeholder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView105"
        android:layout_alignBaseline="@+id/textView104"
        android:layout_alignBottom="@+id/textView104"
        android:layout_alignStart="@+id/textView103"
        android:layout_marginStart="14dp" />

</RelativeLayout>